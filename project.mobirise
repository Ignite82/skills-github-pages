Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#222222\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 col-lg-10\">\n                <div class=\"title-wrapper\" mbr-if=\"showTitle\">\n                    <div class=\"container-color\" mbr-if=\"showContainer\"></div>\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\">\n                        <b>Our Team</b>\n                    </h2>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"item features-image сol-12 col-lg-3 col-md-6\">\n                <div class=\"item-wrapper\">\n                    <div class=\"item-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/istockphoto-1170394707-521x347.jpg\" alt>\n                    </div>\n                    <div class=\"item-content\">\n                        <p class=\"mbr-name mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-name\" mbr-if=\"showName\">&nbsp;</p>\n                        <p class=\"mbr-role mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-role\" mbr-if=\"showRole\">\n                                Director\n                        </p>\n          